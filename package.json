{
  "name": "webpackplatzi",
  "version": "1.0.0",
  "description": "Curso de Webpack en Platzi",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack index.js --output bundle.js --mode development",
    "build:local": "webpack",
    "build:external": "webpack --config ./externalWebPack/webpack.config.js",
    "build:css": "webpack --config ./css-style-loader/webpack.config.js",
    "build:extract:css": "webpack --config ./plugin-extract-text/webpack.config.js",
    "build:multiple": "webpack --config ./multiple-entry-points/webpack.config.js",
    "build:dev": "webpack-dev-server --config ./webpack-dev-server/webpack.config.js",
    "build:babel": "webpack --config ./babel-loader/webpack.config.js",
    "build:images": "webpack --config ./url-loader-images/webpack.config.js",
    "build:fonts": "webpack --config ./url-loader-fonts/webpack.config.js",
    "build:video": "webpack --config ./url-loader-video/webpack.config.js",
    "build:json": "webpack --config ./json-loader/webpack.config.js",
    "build:react": "webpack --config ./react/webpack.config.js",
    "build:sass": "webpack --config ./sass-loader/webpack.config.js",
    "build:stylus": "webpack --config ./stylus-loader/webpack.config.js",
    "build:less": "webpack --config ./less-loader/webpack.config.js",
    "build:postcss": "webpack --config ./postcss-loader/webpack.config.js",
    "build:prevent": "webpack --config ./prevent-duplications/webpack.config.js",
    "build:vendor": "webpack --config ./vendor/webpack.config.js",
    "build:dll": "webpack --config ./dll/webpack.dll.config.js",
    "build:dll:src": "webpack --config ./dll/webpack.config.js",
    "build:dynamic": "webpack --config ./dynamic-imports/webpack.config.js"
  },
  "keywords": [
    "webpack"
  ],
  "author": "@gbrayhan",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "7.0.0",
    "@babel/preset-env": "7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "7.0.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "1.0.0",
    "file-loader": "^2.0.0",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "0.4.2",
    "nib": "^1.1.2",
    "node-sass": "^4.9.3",
    "postcss": "^7.0.2",
    "postcss-cssnext": "^3.1.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^5.3.0",
    "rupture": "^0.7.1",
    "sass-loader": "^7.1.0",
    "style-loader": "0.22.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "url-loader": "1.1.1",
    "webpack": "4.17.1",
    "webpack-cli": "3.1.0",
    "webpack-dev-server": "3.1.6"
  },
  "dependencies": {
    "react": "^16.4.2",
    "react-dom": "^16.4.2"
  }
}
